@model IEnumerable< UserInfo>

@{
    ViewBag.Title = "Toplantı Katılım Listesi";
    Layout = "_Layout2";
}

<h1>Toplantı Katılım Listesi</h1>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Ad</th>
            <th>Email</th>
            <th>Katılım Durumu</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
          
            <tr>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.WillAttend</td>
                <td><a class="btn btn-sm btn-primary" href="#">Detay</a></td>

            </tr>
        }
        }

    </tbody>
</table>